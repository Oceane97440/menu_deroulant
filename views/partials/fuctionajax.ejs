<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script>
$("#users").on("change",()=>{
$.ajax({
    url:"/info/"+$("#users").val(),

    success:(data)=>{
console.log(data)
$("#id").html(data[0].id)
$("#nom").html(data[0].nom)
$("#prenom").html(data[0].prenom)
$("#profession").html("Profession : "+data[0].profession)
$("#age").html("Age : "+data[0].age)
$("#adresse").html("Adresse : "+data[0].adresse)
$("#tel").html("Telephone : "+data[0].tel)

    }
   
})
})
</script>